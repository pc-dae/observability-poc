
{
  "id": null,
  "title": "VictoriaMetrics Overview",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Active Time Series",
      "datasource": "VictoriaMetrics",
      "targets": [
        {
          "expr": "count({__name__!=""})",
          "legendFormat": "Active Series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Storage Used (MB)",
      "datasource": "VictoriaMetrics",
      "targets": [
        {
          "expr": "sum(vm_data_size_bytes) / (1024*1024)",
          "legendFormat": "MB",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "megabytes"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Storage Growth (MB per Hour)",
      "datasource": "VictoriaMetrics",
      "targets": [
        {
          "expr": "increase(sum(vm_data_size_bytes)[1h]) / (1024*1024)",
          "legendFormat": "Increase MB/h",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "megabytes"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    }
  ],
  "templating": {
    "list": []
  }
}
